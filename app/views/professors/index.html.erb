<h1>Faculty Index Page</h1>
<hr>
    <table>
  <thead>
    <tr>
      <th>Professor</th>
      <th>Subject</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
  <%@professors.each do |p|%>
      <tr>
        <td><%= p.name %></td>
        <td><%= p.subject %></td>
        <td><%= link_to 'Show', p %></td>
        <td><%= link_to 'Edit', edit_professor_path(p) %></td>
        <td><%= link_to 'Destroy', p, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>


